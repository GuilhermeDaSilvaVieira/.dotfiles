# Remove any existing config folders that can have a conflict
cd ~/.config/
rm awesome fish kitty nvim picom lf rofi -rf

# Symlink everything
ln -sf ~/.dotfiles/awesome/ ~/.config/awesome
ln -sf ~/.dotfiles/fish/ ~/.config/fish
ln -sf ~/.dotfiles/kitty/ ~/.config/kitty
ln -sf ~/.dotfiles/nvim/ ~/.config/nvim
ln -sf ~/.dotfiles/picom/ ~/.config/picom
ln -sf ~/.dotfiles/lf/ ~/.config/lf
ln -sf ~/.dotfiles/chrome ~/.librewolf/*-release/chrome
